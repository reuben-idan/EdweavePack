# Secure production configuration for EdweavePack
aws_region = "eu-north-1"
environment = "production"
project_name = "edweavepack"

# Security settings
enable_waf = true
enable_secrets_manager = true
enable_encryption = true
ssl_policy = "ELBSecurityPolicy-TLS13-1-2-2021-06"

# Generate secure passwords (replace with actual secure values)
db_password = "CHANGE_ME_SECURE_DB_PASSWORD"
jwt_secret_key = "CHANGE_ME_SECURE_JWT_KEY"